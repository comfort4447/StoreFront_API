    "@types/pg": "^7.14.7",
    "@types/supertest": "^2.0.11",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cross-env": "^7.0.3",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "global": "^4.4.0",
